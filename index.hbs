<!DOCTYPE html>
<html lang="en">
<head>
  <meta charset="UTF-8">
  <meta name="viewport" content="width=device-width, initial-scale=1.0">
  <title>{{siteTitle}}</title>
  <link rel="stylesheet" href="styles.css">
</head>
<body>
  <header>
    <h1>{{siteTitle}}</h1>
  </header>
  <main>
    {{#each items}}
      <article>
        <h2>{{title}}</h2>
        <p>{{description}}</p>
        <time>{{pubDate}}</time>
      </article>
    {{/each}}
  </main>
</body>
</html>